echo "==================Configuring cleartextparser=============================="
while test $# != 0
do
  case $1 in
  --*=*)
    ac_option=`expr "X$1" : 'X\([^=]*\)='`
    ac_optarg=`expr "X$1" : 'X[^=]*=\(.*\)'`
    ac_shift=:
    ;;
  *)
    ac_option=$1
    ac_optarg=$2
    ac_shift=shift
    ;;
  esac
  
	case $ac_option in
  -srcdir=* | --srcdir=* | --srcdi=* | --srcd=* | --src=* | --sr=* | --srcdir | -srcdir)
    srcdir=$ac_optarg ;;
    esac
    
    shift
done

cmake ${srcdir} -DCMAKE_CXX_FLAGS="-mtune=generic -std=c++11" -DCMAKE_BUILD_TYPE=Release
echo "==================Configuring cleartextparser finished====================="

